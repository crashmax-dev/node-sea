{
  "name": "node-sea",
  "type": "commonjs",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run build && ./bin",
    "build": "node --experimental-sea-config sea-config.json && cp $(command -v node) bin && npm run sign",
    "sign": "postject bin NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2"
  },
  "engines": {
    "node": ">=18.16.0 || >=19.7.0"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "postject": "1.0.0-alpha.6"
  }
}
